{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "apiVersion": {
      "type": "string",
      "metadata": {
        "description": "Schema api version from parent template"
      }
    },
    "adPDCNICIPAddress": {
      "type": "string"
    },
    "adBDCNICIPAddress": {
      "type": "string"
    },
    "adBDCNicName": {
      "type": "string"
    },
    "TRZSubnetRef": {
      "type": "string"
    },
    "dnsServers": {
      "type": "array",
      "metadata": {
        "Description": "DNS Servers configured on the NIC"
      }
    }
  },
  "resources": [
    {
      "apiVersion": "[parameters('apiVersion')]",
      "location": "[resourceGroup().location]",
      "name": "[parameters('adBDCNicName')]",
      "type": "Microsoft.Network/networkInterfaces",
      "properties": {
        "ipConfigurations": [
          {
            "name": "ipconfig1",
            "properties": {
              "privateIPAllocationMethod": "Static",
              "privateIPAddress": "[parameters('adBDCNICIPAddress')]",
              "subnet": {
                "id": "[parameters('TRZSubnetRef')]"
              }
            }
          }
        ],
        "dnsSettings": {
          "dnsServers": "[parameters('dnsServers')]"
        }
      }
    }
  ],
  "outputs": {
    "result": {
      "value": "NetworkInterfaces Provisioning achieved",
      "type": "string"
    }
  }
}