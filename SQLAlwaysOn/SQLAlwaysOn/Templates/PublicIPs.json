{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "apiVersion": {
      "type": "string",
      "metadata": {
        "description": "Schema api version from parent template"
      }
    },
    "PIPArrAddressName": {
      "type": "string"
    },
    "PIPAdm1AddressName": {
      "type": "string"
    },
    "PIPAdm2AddressName": {
      "type": "string"
    },
    "PIPGWAddressName": {
      "type": "string"
    }
  },
  "variables": {
    "PIPAddressType": "Dynamic",
    "arrDomainNameLabel": "[toLower(parameters('PIPArrAddressName'))]",
    "adm1DomainNameLabel": "[toLower(parameters('PIPAdm1AddressName'))]",
    "adm2DomainNameLabel": "[toLower(parameters('PIPAdm2AddressName'))]",
    "gwDomainNameLabel": "[toLower(parameters('PIPGWAddressName'))]"
  },
  "resources": [
    
  ],
  "outputs": {
    "PIPArrAddressResourceId": {
      "type": "string",
      "value": "[resourceId('Microsoft.Network/publicIPAddresses',parameters('PIPArrAddressName'))]"
    },
    "PIPAdm1AddressResourceId": {
      "type": "string",
      "value": "[resourceId('Microsoft.Network/publicIPAddresses',parameters('PIPAdm1AddressName'))]"
    },
    "PIPAdm2AddressResourceId": {
      "type": "string",
      "value": "[resourceId('Microsoft.Network/publicIPAddresses',parameters('PIPAdm2AddressName'))]"
    },
    "PIPGWAddressResourceId": {
      "type": "string",
      "value": "[resourceId('Microsoft.Network/publicIPAddresses',parameters('PIPGWAddressName'))]"
    },
    "result": {
      "value": "Public IPs Provisioning achieved",
      "type": "string"
    }
  }
 }